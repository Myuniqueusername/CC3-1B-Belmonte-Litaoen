
package classesandobjects;
import java.util.Scanner;

public class ClassesAndObjects {
    static Scanner sc = new Scanner(System.in);
    
    public static void main(String[] args) {
        
        Dog bantay = new Dog();
        //create another dog object 
    
        System.out.println("Enter name for dog bantay: ");
        String dogName = sc.next();
        bantay.setName(dogName);
        String getName = bantay.getName();
        
        System.out.println("Enter age for " + dogName + ": ");
        int dogAge = sc.nextInt();
        bantay.setAge(dogAge);
        int getAge = bantay.getAge();
        
        System.out.println("Enter breed for " + dogName + ": ");
        String dogBreed = sc.next();
        bantay.setBreed(dogBreed);
        String getBreed = bantay.getBreed();
        
        System.out.println("Enter gender for " + dogName + ": ");
        char dogGender = sc.next().charAt(0);
        bantay.setGender(dogGender);
        char getGender = bantay.getGender();
        
     

     
     Cardo Dalisay = new Cardo();
     
     System.out.println("Enter name for dog cardo: ");
        String cardoName = sc.next();
        Dalisay.setName(cardoName);
        String a = Dalisay.getName();
        
        System.out.println("Enter age for " + cardoName + ": ");
        int cardoAge = sc.nextInt();
        Dalisay.setAge(cardoAge);
        int b = Dalisay.getAge();
        
        System.out.println("Enter breed for " + cardoName + ": ");
        String cardoBreed = sc.next();
        Dalisay.setBreed(cardoBreed);
        String c = Dalisay.getBreed();
        
        System.out.println("Enter gender for " + cardoName + ": ");
        char cardoGender = sc.next().charAt(0);
        Dalisay.setGender(cardoGender);
        char d = Dalisay.getGender();
     

     System.out.println("");
     System.out.println("Bantay ");
     System.out.println("Name: " + getName);
     System.out.println("Age: " + getAge);
     System.out.println("Breed: " + getBreed);
     System.out.println("Gender: " + getGender);
     System.out.println("");
     System.out.println("Cardo ");
     System.out.println("Name: " + a);
     System.out.println("Age: " + b);
     System.out.println("Breed: " + c);
     System.out.println("Gender: " + d);
     System.out.println("");
    }
    
}



package classesandobjects;
import java.util.Scanner;

public class Dog {
    String name;
    int age;
    String breed;
    char gender;
    
   


    public void setName(String name){
        this.name = name;
    }
    public String getName(){
        return name;
    }

    
    
    public void setAge(int age){
         this.age = age;
}
    public int getAge(){
         return age;
}


    public void setBreed(String breed){
         this.breed = breed;
}
    public String getBreed(){
         return breed;
        
}
    
    
    public void setGender(char gender){
         this.gender = gender;
}
    public char getGender(){
         return gender;
}
}


